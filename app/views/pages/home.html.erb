
<div class="landing-splash d-flex flex-column justify-content-start align-items-center" style="background-image: url('http://res.cloudinary.com/drxcsc3ye/image/upload/v1693476762/purple_background.png');">
  <div class="landing-offset"></div>
  <div>
    <%= render 'layouts/search_form' %>
  </div>
</div>
<div class="container pt-3 scrollable">
  <hr class="py-2">
  <div class="title-center">
    <h1>local jamsters</h1>
  </div>
  <% @profiles.each do |profile| %>
    <%= link_to profile_path(profile) do %>
      <%= render "layouts/user_summary", user: profile %>
    <% end %>
  <% end %>
</div>
<%= render 'shared/footer' %>
